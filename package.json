    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.16.3",
    "prisma": "^4.13.0",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.4",
